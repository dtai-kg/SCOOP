@prefix ex: <http://example.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/NodeShape/address> a sh:NodeShape ;
    sh:name "address" ;
    sh:nodeKind sh:IRI ;
    sh:property <file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/USAddress/country>,
        <file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/USAddress/street>,
        <file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/address>,
        <http://example.com/PropertyShape/USAddress/country2> ;
    sh:targetClass ex:address .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/NodeShape/test> a sh:NodeShape ;
    sh:name "test" ;
    sh:nodeKind sh:IRI ;
    sh:property <file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/test> ;
    sh:targetClass ex:Test .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/USAddress/country> a sh:PropertyShape ;
    sh:datatype xsd:NMTOKEN ;
    sh:in ( "US" "DK" "BE" ) ;
    sh:name "country" ;
    sh:path ex:country .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/USAddress/street> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 5 ;
    sh:minCount 2 ;
    sh:minLength 5 ;
    sh:name "street" ;
    sh:path ex:street .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/address> a sh:PropertyShape ;
    sh:maxCount 7 ;
    sh:minCount 3 ;
    sh:name "address" ;
    sh:path ex:address ;
    sh:targetSubjectsOf ex:address .

<file:///D:/xueminduan/PhD/projects/Shapes-Integration/test_case/PropertyShape/test> a sh:PropertyShape ;
    sh:name "test" ;
    sh:path ex:test .

<http://example.com/PropertyShape/USAddress/country2> a sh:PropertyShape ;
    sh:name "test" ;
    sh:path ex:country2 ;
    sh:targetSubjectsOf ex:country2 .

