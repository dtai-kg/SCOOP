@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://example.com/NodeShape/> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:property <http://example.com/PropertyShape/etcsBaseline> .

<http://example.com/NodeShape/ETCSLevel> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:property <http://example.com/PropertyShape/etcsLevelType>,
        <http://example.com/PropertyShape/rdf-schema#label> ;
    sh:targetClass <http://data.europa.eu/949/ETCSLevel> .

<http://example.com/PropertyShape/etcsBaseline> a sh:PropertyShape ;
    sh:path <http://data.europa.eu/949/etcsBaseline> .

<http://example.com/PropertyShape/etcsLevelType> a sh:PropertyShape ;
    sh:path <http://data.europa.eu/949/etcsLevelType> .

<http://example.com/PropertyShape/rdf-schema#label> a sh:PropertyShape ;
    sh:path rdfs:label .

