@prefix ex: <http://example.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:PersonShape
	a sh:NodeShape ;
	sh:targetClass ex:Person ;    
	sh:property [                
		sh:path ex:age ;        
        sh:minExclusive 60 ;    
		sh:datatype xsd:integer ;
	] .

