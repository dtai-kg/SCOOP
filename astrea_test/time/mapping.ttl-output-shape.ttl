<http://mapping.example.com/map_triplesMapInterval_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:node <http://mapping.example.com/map_triplesMapInstant_000/shape> ;
            sh:nodeKind sh:IRI ;
            sh:path ns:inside ] ;
    sh:targetClass <http://www.w3.org/2006/time#Interval> .

